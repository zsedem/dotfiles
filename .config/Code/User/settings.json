{
    "editor.fontFamily": "'Monofur Nerd Font Mono','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 20,
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.autoUpdate": false,
    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "helm-intellisense.customValueFileNames": [
        "prod.yaml",
        "jenkins.yaml",
        "values.yaml"
    ],
    "metals.ammoniteJvmProperties": [
        "-Xmx8G"
    ],
    "npm.fetchOnlinePackageInfo": false,
    "telemetry.telemetryLevel": "error",
    "terminal.integrated.fontSize": 17,
    "update.mode": "none",
    "workbench.colorTheme": "Solarized Light",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "editor.tabSize": 2,
    "extensions.ignoreRecommendations": true,
    "git.autofetch": true,
    "redhat.telemetry.enabled": false,
    "workbench.startupEditor": "none",
    "metals.javaHome": "/run/current-system/sw/lib/openjdk17",
    "json.schemas": [],
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "aws.telemetry": false,
    "yaml.validate": false,
    "files.associations": {
        "*.json": "json"
    },
    "cSpell.userWords": [
        "addrs",
        "aggr",
        "Akka",
        "akkahttp",
        "akkastreams",
        "Appendable",
        "Archiver",
        "badrequest",
        "Behaviour",
        "bgcolor",
        "buildinfo",
        "Builtins",
        "CELLBORDER",
        "CELLPADDING",
        "CELLSPACING",
        "Charsets",
        "Chrono",
        "classfication",
        "clazz",
        "Cloneable",
        "clsx",
        "Codegenerator",
        "Coproduct",
        "dconfig",
        "defaultde",
        "demov",
        "deps",
        "Deserialization",
        "endregion",
        "Enumeratum",
        "expl",
        "fastparse",
        "flatfile",
        "fontcolor",
        "fundef",
        "hientries",
        "Hindley",
        "historyinfo",
        "implicits",
        "ingestor",
        "ipaddress",
        "Jdbc",
        "kubernetes",
        "logback",
        "logit",
        "Managment",
        "materializer",
        "Metavar",
        "Milner",
        "munit",
        "NANP",
        "pprint",
        "Productv",
        "pureconfig",
        "Reloadable",
        "rulename",
        "scalacache",
        "scalacheck",
        "Scalafmt",
        "Scalatest",
        "Segmentations",
        "Serializers",
        "testkit",
        "typeclass",
        "Typesafe",
        "ujson",
        "Unmarshal",
        "unmarshaller",
        "Xfatal",
        "Ymacro"
    ],
    "editor.inlayHints.enabled": false,
    "editor.formatOnSave": true,
    "metals.enableStripMarginOnTypeFormatting": false,
    "metals.showImplicitArguments": true,
    "metals.showInferredType": true,
    "editor.unicodeHighlight.allowedCharacters": {
        "â€™": true
    },
    "workbench.preferredDarkColorTheme": "Visual Studio Light",
    "workbench.preferredHighContrastColorTheme": "Visual Studio Light",
    "diffEditor.renderSideBySide": false,
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.codeLens": true,
    "gitlens.hovers.avatars": false,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "properties": false
    },
    "githubPullRequests.pushBranch": "always",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "security.workspace.trust.untrustedFiles": "newWindow"
}
