{
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "'monofur','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 20,
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.autoUpdate": false,
    "files.autoSave":"onFocusChange",
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "helm-intellisense.customValueFileNames": [ "prod.yaml", "../../commons/tpls.yaml", "jenkins.yaml", "values.yaml" ],
    "json.schemaDownload.enable": false,
    "metals.ammoniteJvmProperties": [ "-Xmx8G" ],
    "npm.fetchOnlinePackageInfo": false,
    "telemetry.telemetryLevel": "error",
    "terminal.integrated.fontSize": 17,
    "update.mode": "none",
    "workbench.colorTheme": "Solarized Dark",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "editor.tabSize": 2,
    "extensions.ignoreRecommendations": true,
    "git.autofetch": true,
    "redhat.telemetry.enabled": false,
    "metals.javaHome": "/nix/var/nix/gcroots/customs/openjdk8/lib/openjdk",
    "workbench.startupEditor": "none"
}
